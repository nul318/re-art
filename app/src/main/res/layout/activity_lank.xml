<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/group_4"
    tools:context=".activities.MypageActivity">
    <ImageView
        android:layout_marginLeft="19dp"
        android:layout_marginTop="11dp"
        android:layout_width="93dp"
        android:layout_height="39dp"
        android:src="@drawable/ic_title"/>
    <ImageView
        android:id="@+id/ic_money"
        android:src="@drawable/ic_money"
        android:layout_alignParentRight="true"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginTop="17dp"
        android:layout_marginRight="55dp"
        />
    <TextView
        android:layout_marginTop="16dp"
        android:layout_marginRight="20dp"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="22sp"
        android:textColor="#456256"
        android:gravity="center_horizontal"
        android:text="60"
        />
    
<ImageView
    android:layout_marginLeft="35dp"
    android:layout_marginTop="81dp"
    android:layout_width="104dp"
    android:layout_height="46dp"
    android:src="@drawable/ic_1"/>
    <ImageView
        android:layout_marginLeft="153dp"
        android:layout_marginTop="81dp"
        android:layout_width="104dp"
        android:layout_height="46dp"
        android:src="@drawable/ic_2"/>
    <ImageView
        android:layout_alignParentRight="true"
        android:layout_marginTop="81dp"
        android:layout_width="104dp"
        android:layout_height="46dp"
        android:src="@drawable/ic_3"/>
<ImageView
    android:id="@+id/iv_ranking"
    android:layout_marginTop="139dp"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:scaleType="centerCrop"
    android:src="@drawable/group_10"/>
<LinearLayout
    android:layout_alignParentBottom="true"
    android:background="#ffffff"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="380dp">
    <TextView
        android:layout_marginLeft="15dp"
        android:layout_marginTop="35dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:textColor="#000000"
        android:gravity="center_horizontal"
        android:text="6월 1째주 글 랭킹"
        />
<ImageView
    android:scaleType="centerCrop"
    android:layout_marginTop="28dp"
    android:src="@drawable/group_9"
    android:layout_width="330dp"
    android:layout_height="323dp"
    android:layout_gravity="center_horizontal"/>
</LinearLayout>


</RelativeLayout>